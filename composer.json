{
    "name": "yarad/laravel-notion-errors-handler",
    "description": "Laravel package for handling exceptions and sending them to Notion database",
    "keywords": ["laravel", "notion", "exception", "error-handling", "logging"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yarad",
            "email": "yarad@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "mariosimao/notion-sdk-php": "^1.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yarad\\NotionExceptionHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yarad\\NotionExceptionHandler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yarad\\NotionExceptionHandler\\NotionExceptionHandlerServiceProvider"
            ],
            "aliases": {
                "NotionExceptionHandler": "Yarad\\NotionExceptionHandler\\Facades\\NotionExceptionHandler"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
